#for importing values from standard library
import random
courses = ['History', 'math', 'science', 'english']

random_choice = random.choice(courses)
print(random_choice)


# importing math module
 
import math
courses = ['History', 'math', 'science', 'english']
rads = math.radians(90)
print(math.sin(rads))
print(math.cos(rads))

#importing date time and calender

import datetime
